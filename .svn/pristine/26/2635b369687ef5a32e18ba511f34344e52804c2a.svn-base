									that.orContinusZhezhao = true
									that.moreCard = true
									that.oneCard = false
									that.deallineday = j
									that.deallinecardNum = res.data.data[j].num
									//									console.log(j, res.data.data[j].num)
									that.deallineNum = res.data.data[j].num
									break
								} else { //只有一张会员卡到期
									that.orContinusZhezhao = true
									that.moreCard = false
									that.oneCard = true
									that.cardType = res.data.data[j].name
									that.deallineday = res.data.data[j].num
									//									console.log(res.data.data[j].name, res.data.data[j].num)
									break
